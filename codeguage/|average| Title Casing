function toTitleCase(str){
    let word = str.split(' ');
    let result = '';

    for(let i = 0; i < word.length; i++){
        if(i != word.length - 1){
            result+= word[i].charAt(0).toUpperCase() + word[i].substring(1) + " ";
        } else{
            result+= word[i].charAt(0).toUpperCase() + word[i].substring(1);
        }
    
    }

    return result;
    
} 
